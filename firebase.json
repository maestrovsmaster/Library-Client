{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "host": "0.0.0.0",
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "library-541e4",
          "appId": "1:15417871659:android:f88fe433b32940d98dc96e",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "library-541e4",
          "appId": "1:15417871659:ios:67a3d693144e3a888dc96e",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "library-541e4",
          "configurations": {
            "android": "1:15417871659:android:f88fe433b32940d98dc96e",
            "ios": "1:15417871659:ios:67a3d693144e3a888dc96e"
          }
        }
      }
    }
  },
  "storage": {
    "rules": "storage.rules"
  }
}
